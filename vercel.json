{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "PORT": "3000"
    },
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "startCommand": "npm start"
}
